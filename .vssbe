    {
      "Enabled": true,
      "Name": "MgrInit",
      "Caption": "Preparing Manager ...",
      "IgnoreIfBuildFailed": true,
      "Process": {
        "$type": "net.r_eg.vsSBE.Events.EventProcess, vsSolutionBuildEvent",
        "Waiting": true,
        "Hidden": false,
        "TimeLimit": 400
      },
      "Mode": {
        "$type": "net.r_eg.vsSBE.Events.ModeScript, vsSolutionBuildEvent",
        "Type": "Script",
        "Command__": [
          "#[IO writeLine(STDOUT): Build hMSBuild and GetNuTool ...]",
          "#[IO call(\"cmd\", \"/C cd hMSBuild & build\", 300)]",
          "",
          "#[var frontend = $(pManager)\\\\batch\\\\Manager.bat]",
          "",
          "",
          "#[$(hCore = 'hMSBuild\\bin\\Release\\hMSBuild.')]",
          "#[$(embdCore = \"$(hCore)embd\")]",
          "#[IO copy.file(\"$(hCore)bat\", \"$(embdCore)\", true)]",
          "",
          "#[var dxpWZ = $(dbin.Replace('/', '\\\\'))DllExport.bat]",
