<?xml version="1.0" encoding="utf-8"?>
<!-- https://github.com/3F/DllExport -->
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  
  <Target Name="DllExportWizardMod">
    
    <PropertyGroup>
      <DllExportWizardSlnDir Condition="'$(DllExportWizardSlnDir)' == ''">.\</DllExportWizardSlnDir>
      <DllExportWizardPkgPath Condition="'$(DllExportWizardPkgPath)' == ''">packages/DllExport</DllExportWizardPkgPath>
      <DllExportWizardAction Condition="'$(DllExportWizardAction)' == ''">Install</DllExportWizardAction> <!-- Default, Configure, Install, Restore, Remove -->
    </PropertyGroup>
    <DllExportCfgTask SlnDir="$(DllExportWizardSlnDir)" PkgPath="$(DllExportWizardPkgPath)" Action="$(DllExportWizardAction)" />
  
  </Target>
  <UsingTask TaskName="net.r_eg.DllExport.Wizard.DllExportCfgTask" AssemblyFile="net.r_eg.DllExport.Wizard.dll" />
  
</Project>